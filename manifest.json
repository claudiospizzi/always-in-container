{
  "manifest_version": 2,
  "name": "Always In Container 2",
  "description": "__MSG_extensionDescription__",
  "version": "2.1.0",
  "default_locale": "en",
  "icons": {
    "48": "icon.svg"
  },
  "permissions": [
    "<all_urls>",
    "contextualIdentities",
    "cookies",
